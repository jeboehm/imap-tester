{
  "name": "jeboehm/imap-tester",
  "description": "Tests imap and pop3 mailboxes.",
  "require": {
    "php": "^8.0",
    "symfony/console": "~5.4",
    "bytestream/horde-imap-client": "^2.31"
  },
  "license": "MIT",
  "autoload": {
    "psr-0": {
      "": "src/"
    }
  },
  "scripts": {
    "build": [
      "vendor/bin/box --version",
      "php -d phar.readonly=off vendor/bin/box compile"
    ],
    "csfix": "vendor/bin/php-cs-fixer fix --allow-risky=yes",
    "rector": "vendor/bin/rector"
  },
  "require-dev": {
    "rector/rector": "^0.13.3",
    "humbug/box": "^3.16.0",
    "php-cs-fixer/shim": "^3.8.0"
  }
}
