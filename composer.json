{
  "name": "jeboehm/imap-tester",
  "description": "Tests imap and pop3 mailboxes.",
  "require": {
    "php": "^8.0",
    "ext-imap": "*",
    "symfony/console": "~5.4",
    "php-imap/php-imap": "~5.0"
  },
  "license": "MIT",
  "autoload": {
    "psr-0": {
      "": "src/"
    }
  },
  "scripts": {
    "build": [
      "tools/box --version",
      "php -d phar.readonly=off tools/box compile"
    ],
    "csfix": "tools/php-cs-fixer fix --allow-risky=yes",
    "rector": "rector"
  },
  "require-dev": {
    "rector/rector": "^0.13.3"
  }
}
